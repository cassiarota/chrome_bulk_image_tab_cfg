{
    "manifest_version": 3,
    "name": "Bulk URL Opener",
    "version": "1.0.0",
    "description": "Right-click on selected text to open all URLs in new tabs",
    "permissions": [
        "contextMenus",
        "tabs",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_title": "Bulk URL Opener"
    }
}